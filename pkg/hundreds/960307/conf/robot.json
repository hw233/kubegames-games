    "bettime": [
        1000,
        6000
    ],
	//下注权重对应rbwarconfig.json中的betamount
    "betweight": [
        70,
        20,
        5,
        3,
        0
    ],
	//下注的地方，第一个表示下红或者黑的权重，第二个表示下幸运一击的权重
    "betplace": [
        80,
        10
    ],
